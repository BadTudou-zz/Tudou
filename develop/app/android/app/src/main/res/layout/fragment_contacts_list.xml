<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:fab="http://schemas.android.com/apk/res-auto"
    tools:context="com.badtudou.view.fragment.ContactsListFragment">
    <!-- TODO: Update blank fragment expandable_selector -->

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical">
        <include
            layout="@layout/app_bar_contacts_list"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />
        <android.support.design.widget.CoordinatorLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">
            <include
                layout="@layout/contacts_list_view"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
            <include
                layout="@layout/floating_action_menu"
                android:layout_width="match_parent"
                android:layout_height="match_parent" />
        </android.support.design.widget.CoordinatorLayout>
    </LinearLayout>


</FrameLayout>
